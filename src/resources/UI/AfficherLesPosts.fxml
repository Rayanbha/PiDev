<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ContextMenu?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="536.0" prefWidth="571.0" styleClass="column-header-background" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.Forum.AfficherLesPosts">

   <children>
      <ListView fx:id="listView" editable="true" layoutX="1.0" layoutY="81.0" prefHeight="406.0" prefWidth="578.0" style="-fx-background-color: #121748;">
         <contextMenu>
            <ContextMenu fx:id="contextMenu">
              <items>
                <MenuItem mnemonicParsing="false" onAction="#editItem" text="Unspecified Action" />
                  <MenuItem mnemonicParsing="false" onAction="#deleteItem" text="Unspecified Action" />
              </items>
            </ContextMenu>
         </contextMenu></ListView>
      <Label alignment="CENTER" contentDisplay="CENTER" graphicTextGap="6.0" prefHeight="77.0" prefWidth="578.0" style="-fx-background-color: #FFDE59;" text="SELECT ME " textFill="#121748">
         <font>
            <Font name="System Bold" size="30.0" />
         </font>
      </Label>
      <Button layoutX="424.0" layoutY="490.0" mnemonicParsing="false" onAction="#detail" prefHeight="36.0" prefWidth="154.0" styleClass="button-custom" text="Plus de details" />

      <ImageView fitHeight="168.0" fitWidth="80.0" layoutX="514.0" layoutY="-45.0" pickOnBounds="true" preserveRatio="true" styleClass="image-view">
         <image>
            <Image url="@logo.png" />
         </image>
      </ImageView>


   </children>
   <stylesheets>
      <URL value="@style.CSS" />
   </stylesheets>
</AnchorPane>
